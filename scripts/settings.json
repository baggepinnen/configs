    "editor.fontFamily": "JuliaMono",
    "editor.fontLigatures": "'zero', 'ss01', 'ss02', 'ss03', 'ss04','ss05', 'ss06',  'ss07', 'ss08', 'ss09', 'ss10'",
    "git.confirmSync": false,
    "git.autofetch": true,
