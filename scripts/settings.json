    "latex-workshop.view.pdf.viewer": "external",
    "juliaFormatter.whitespaceTypedefs": true,
    "autoalign.minSeparationLeft": 1,
    "alignment.surroundSpace": {
        "assignment": [
            1,
            1
        ],
        "arrow": [
            1,
            1
        ],
        "comment": 2
    },
    "markdown-pdf.scale": 0.8,
    "markdown-pdf.margin.left": "2cm",
    "markdown-pdf.margin.right": "2cm",
    "markdown-pdf.margin.top": "2.5cm",
